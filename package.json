{
  "name": "ecommerce-event-processor",
  "version": "0.0.1",
  "description": "Real-time event processing system for e-commerce",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/main.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@types/sequelize": "^4.28.20",
    "amqplib": "^0.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "inversify": "^7.5.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "reflect-metadata": "~0.2.2",
    "sequelize": "^6.37.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "nodejs",
    "rabbitmq",
    "clean-architecture",
    "ddd",
    "solid",
    "ecommerce",
    "event-processing"
  ],
  "author": "E-commerce Team",
  "license": "MIT"
}
